<div class="user-profile">
  <div class="profile row">
    <div class="col-md-4 avatar">
      <div class="wrapper">
        <img <%= image_tag("harry_profile_pic") %> 
      </div>
    </div>
    
    <div class="col-md-8 basic-info">
      <div class="user-wrapper">
        <h2 class="username">HarryPotter</h2>
        <a class="edit-profile" href="<%= edit_user_path(current_user) %>">
          <button>Edit Profile</button>
        </a>
      </div>

      <ul class="posts">
        <li><span>12</span> posts</li>
        <li><span>195</span> followers</li>
        <li><span>345</span> following</li>
      </ul>

      <div class="user-tabs">
        <a class="tab active" href="">
          <i class="fa fa-th"></i>
          POSTS
        </a>
        <a class="tab" href="">
          <i class="fa fa-bookmark"></i>
          SAVED
        </a>
        <a class="tab" href="">
          <i class="fa fa-tag"></i>
          TAGGED
        </a>
      </div>

      <h2 class="name">Harry Potter</h2>
      <a class="website">harrypotter.com</a>
    </div>
  </div>

    <div class="user-images">
      <div class="wrapper">
        <img <%= image_tag("gf") %> 
      </div>
      <div class="wrapper">
        <img <%= image_tag("sorting_hat") %> 
      </div>
      <div class="wrapper">
        <img <%= image_tag("throw_back") %> 
      </div>
    </div>
</div>
